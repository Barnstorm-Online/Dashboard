{
  "name": "barnstorm-online",
  "version": "1.0.0",
  "description": "Get Active",
  "license": "MIT",
  "productName": "Barnstorm Online",
  "cordovaId": "online.barnstorm.dashboard",
  "author": "Michael J Feher <github@phearzero.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0",
    "build": "yarn build-desktop & yarn build-mobile",
    "build-ssr-pwa": "yarn build-ssr & yarn build-pwa",
    "build-ssr": "quasar build -m ssr",
    "build-pwa": "quasar build -m pwa",
    "build-desktop": "quasar build --mode electron",
    "build-mobile": "yarn build-android & yarn build-ios",
    "build-android": "quasar build --mode cordova -T android -t mat",
    "build-ios": "quasar build --mode cordova -T ios -t ios"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "devtron": "1.4.0",
    "electron": "2.0.0",
    "electron-builder": "^20.28.4",
    "electron-debug": "1.5.0",
    "electron-devtools-installer": "2.2.4",
    "electron-packager": "^12.0.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "quasar-cli": "^0.17.0",
    "strip-ansi": "=3.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
